import sys
import math
class Point():
    def __init__(self,x,y):
        self.x = x
        self.y = y
    def distance(self):
        return math.sqrt(self.x**2 + self.y**2)
class Triangle():
    def __init__(self,side1,side2,side3):
        self.side1 = side1
        self.side2 = side2
        self.side3 = side3
    def check_triangle(self):
        return self.side1 <= (self.side2 + self.side3) and self.side2 <= (self.side1 + self.side3) and self.side3 <= (self.side2 + self.side1)
def readfile(filename):
    f = open(filename)
    return [i for i in f.readline().split()]
def check_nums(coordinates):
    if len(coordinates) != 6:
        return False
    for i in coordinates:
        if not i.isdigit() and i[0] != '-' and not i[1].isdigit():
            return False
    return True
nums = readfile(sys.argv[1])
if not check_nums(nums):
    print('координаты с ошибкой')
else:
    nums = [float(num) for num in nums]
    point1 = Point(nums[0], nums[1])
    point2 = Point(nums[2],nums[3])
    point3 = Point(nums[4],nums[5])
